{
  "name": "WebPagetest-lite",
  "version": "0.1",
  "description": "Automates testing of web performance",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "WebPagetest-lite",
    "default_icon": "icon-popup.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "browsingData",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "icons": { 
	"128": "icon-128.png", 
	"48": "icon-48.png"
  },
  "web_accessible_resources": [
    "blank.html"
  ],
  "manifest_version": 2
}
